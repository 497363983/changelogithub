    "lodash.merge": "^4.0.2",
    "meow": "^7.0.0",
    "q": "^1.4.1",
    "semver": "^5.0.1",
    "semver-regex": "^2.0.0",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",

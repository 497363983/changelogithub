{
  "name": "changelogithub",
  "version": "0.0.2-beta.1",
  "packageManager": "pnpm@7.2.0",
  "description": "Generate changelog for GitHub.",
  "author": "Steve Mao <maochenyan@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/antfu/changelogithub",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/changelogithub.git"
  },
  "bugs": "https://github.com/antfu/changelogithub/issues",
  "keywords": [
    "changelogithub",
    "github",
    "release",
    "releases",

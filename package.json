    "lodash.merge": "^4.6.2",
    "meow": "^10.1.2",
    "q": "^2.0.3",
    "semver": "^7.3.7",
    "semver-regex": "^4.0.5",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
